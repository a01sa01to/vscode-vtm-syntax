{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "virtual-turing-machine",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#state"
		}
	],
	"repository": {
		"comment": {
			"patterns": [
				{
					"name": "comment",
					"match": "^#(.*)$"
				}
			]
		},
		"state": {
			"patterns": [
				{
					"name": "entity.name.function",
					"match": "^(?!.*\\s-\\s).*$"
				}
			]
		}
	},
	"scopeName": "source.vtm"
}