{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "virtual-turing-machine",
	"patterns": [
		{
			"include": "#comment"
		}
	],
	"repository": {
		"comment": {
			"patterns": [
				{
					"name": "comment",
					"match": "^#(.*)$"
				}
			]
		}
	},
	"scopeName": "source.vtm"
}